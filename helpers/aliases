alias               tt='while true;do clear;t;sleep 2s;done'
alias               ls='ls -GFh'
alias              vim='/usr/bin/vi'
alias         dotfiles="cd ${HOME}/.dotfiles"
alias dotfiles-helpers="cd ${HOME}/.dotfiles_helpers"
alias dotfiles-scripts="cd ${HOME}/.dotfiles_helpers/scripts"
alias dotfiles-aliases="vi ${HOME}/.dotfiles_helpers/aliases"
